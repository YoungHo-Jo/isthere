<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.apptive.joDuo.isthere">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.apptive.joDuo.isthere.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ReviewMain"/>
        <activity android:name=".SearchCategory"/>
        <activity android:name=".MakeReview"/>
        <activity android:name=".LikeReview"/>
        <activity android:name=".Setting"/>
        <activity android:name=".LoginPage"/>
        <activity android:name=".RegisterLogin"/>
        <activity android:name=".PresentLocation"/>
    </application>


</manifest>